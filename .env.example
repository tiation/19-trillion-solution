# The $19 Trillion Solution - Environment Configuration
# Tiation & ChaseWhiteRabbit Initiative

# ====================================
# Application Configuration
# ====================================

# Environment (development, staging, production)
VITE_NODE_ENV=development

# Application Base URL
VITE_APP_BASE_URL=https://tiation.github.io/19-trillion-solution/

# API Configuration
VITE_API_BASE_URL=https://api.tiation.org/v1
VITE_API_TIMEOUT=30000

# ====================================
# Feature Flags
# ====================================

# Enable experimental features
VITE_ENABLE_EXPERIMENTAL_FEATURES=false

# Enable analytics
VITE_ENABLE_ANALYTICS=true

# Enable error reporting
VITE_ENABLE_ERROR_REPORTING=true

# ====================================
# Analytics & Monitoring
# ====================================

# Google Analytics (if enabled)
VITE_GA_TRACKING_ID=

# Sentry DSN (if enabled)
VITE_SENTRY_DSN=

# Application Insights (if enabled)
VITE_APP_INSIGHTS_KEY=

# ====================================
# Social & External Services
# ====================================

# Social media links
VITE_SOCIAL_TWITTER=https://twitter.com/tiation
VITE_SOCIAL_LINKEDIN=https://linkedin.com/company/tiation
VITE_SOCIAL_GITHUB=https://github.com/tiation

# ChaseWhiteRabbit links
VITE_CHASEWHITERABBIT_WEBSITE=https://chasewhiterabbit.org
VITE_CHASEWHITERABBIT_GITHUB=https://github.com/tiation/ChaseWhiteRabbit

# ====================================
# Content Management
# ====================================

# CMS Configuration (if using headless CMS)
VITE_CMS_ENDPOINT=
VITE_CMS_API_KEY=

# ====================================
# Development & Testing
# ====================================

# Mock API responses in development
VITE_MOCK_API=false

# Enable debug mode
VITE_DEBUG=false

# Testing environment
VITE_TEST_MODE=false

# ====================================
# Security
# ====================================

# Content Security Policy (CSP) settings
VITE_CSP_REPORT_URI=https://api.tiation.org/csp-report

# Rate limiting
VITE_RATE_LIMIT_REQUESTS=100
VITE_RATE_LIMIT_WINDOW=3600

# ====================================
# Contact Information
# ====================================

# Support email
VITE_SUPPORT_EMAIL=support@tiation.org

# Security email
VITE_SECURITY_EMAIL=security@tiation.org

# General contact
VITE_CONTACT_EMAIL=info@tiation.org

# ====================================
# Build Configuration
# ====================================

# Build optimization
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true

# PWA Configuration
VITE_PWA_ENABLED=true
VITE_PWA_OFFLINE_ENABLED=true

# ====================================
# Performance Monitoring
# ====================================

# Performance tracking
VITE_PERFORMANCE_MONITORING=true

# Web vitals reporting
VITE_WEB_VITALS_ENDPOINT=https://api.tiation.org/web-vitals

# ====================================
# Accessibility
# ====================================

# Accessibility features
VITE_A11Y_ENABLED=true
VITE_A11Y_ANNOUNCE_ROUTE_CHANGES=true

# ====================================
# Internationalization
# ====================================

# Default language
VITE_DEFAULT_LOCALE=en-AU

# Supported languages
VITE_SUPPORTED_LOCALES=en-AU,en-US,en-GB

# ====================================
# Cache Configuration
# ====================================

# Cache duration (in seconds)
VITE_CACHE_DURATION=3600

# Service worker cache version
VITE_SW_CACHE_VERSION=1.0.0

# ====================================
# Development Tools
# ====================================

# React Developer Tools
VITE_REACT_DEVTOOLS=true

# Redux DevTools
VITE_REDUX_DEVTOOLS=true

# Bundle analyzer
VITE_BUNDLE_ANALYZER=false
